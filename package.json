{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --setupFilesAfterEnv ./tests/setup.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "nodemon --watch src --exec ts-node src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "lint": "eslint src --ext .ts",
    "migration:generate": "npx typeorm migration:generate -d dist/db-config.js",
    "migration:run": "npx typeorm migration:run -d dist/db-config.js",
    "migration:revert": "npx typeorm migration:revert -d dist/db-config.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/express": "5.0.3",
    "axios": "1.7.2",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "exceljs": "4.4.0",
    "express": "4.21.2",
    "node-cron": "4.2.1",
    "pg": "8.16.2",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.25"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@types/cors": "2.8.19",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.3",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "cross-env": "10.0.0",
    "eslint": "9.30.1",
    "globals": "16.2.0",
    "jest": "30.1.3",
    "nodemon": "3.1.10",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1"
  }
}
