    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.25"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/cors": "2.8.19",
    "@types/node": "24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
